{
    "name": "submtd/laravel-webhooks-client",
    "description": "Client package for Laravel Webhooks",
    "authors": [
        {
            "name": "Steve Harmeyer",
            "email": "harmeyersteve@gmail.com"
        }
    ],
    "require": {
        "dyrynda/laravel-model-uuid": "^5.0",
        "spatie/laravel-json-api-paginate": "^1.5",
        "submtd/laravel-http-request": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Submtd\\LaravelWebhooksClient\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Submtd\\LaravelWebhooksClient\\Providers\\WebhooksClientServiceProvider"
            ],
            "aliases": {
                "WebhooksClient": "Submtd\\LaravelWebhooksClient\\Facades\\WebhooksClient"
            }
        }
    }
}
